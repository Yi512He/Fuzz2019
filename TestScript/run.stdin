TESTS=( t1 t2 t3 t4 t5 t6 t7 t8 t9 t10 t11 t12 )
NOTES=notesNew

echo Testing $* ... >> $NOTES

for test in ${TESTS[*]}
    # onintr InfiniteLoop
     do
       echo $1" <"$test">&" "/dev/null"
       #$1 <$test >& /dev/null
       $1 <$test>& /dev/null
       trap 'echo "time out" echo "infinite loop detected " $test >> $NOTES' 2
       if [ $? -eq 139 ]; then
          echo "core dumped"
          echo "core dump detected " $test >> $NOTES
       fi
     done
